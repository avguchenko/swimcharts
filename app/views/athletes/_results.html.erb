<table id="results_table" class="table table-striped table-condensed">
	<thead>
		<tr>
			<th>id</th>
			<th>nation</th>
			<th>date</th>
			<th>course</th>
			<th>event (round)</th>

			<th>place</th>
			<th span="1">swim time</th>
			<th>reaction time</th>
		</tr>
	</thead>

	<tbody>
		<% for result in @results -%>
			<tr>
				<td><%=link_to result.id, result%></td>
				<td><%=result.club.nation%></td>
				<td><abbr title="<%=result.meet.name%>"><%=result.event.session.session_date.strftime("%Y-%m-%d")%></abbr></td>
				<td><%=result.meet.course%></td>
				<td><%=result.swim_style.to_s%> (<%=result.event.round%>)</td>
				<td><%=result.place%></td>
				<td>

				<div id="pop_<%=result.id%>" class="label" rel="popover" data-content="" data-original-title="Splits"><%=result.swimtime%></div>

			</td>
				<td><%=result.reactiontime%></td>

			</tr>

		<%end%>

	</tbody>


</table>